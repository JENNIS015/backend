<div class="table-responsive">
  <div id="contenedor">
   
    {{#if productos.length}}
   <h2 style="color: blue">Vista de Productos</h2>
      <div class="table-responsive">
        <table class="table table-dark">
          <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Foto URL</th>
          </tr>
          <!-- filas -->
          {{#each productos}}
            <tr>
              <td>{{nombre}}</td>
              <td>{{precio}}</td>
              <td>
                <img
                  style="display: inline-flex; width: 40px; height: 40px"
                  src="{{foto}}"
                />
              </td>
            </tr>
          {{/each}}
        </table>
      </div>
    {{else}}

    {{/if}}

  </div>
</div>