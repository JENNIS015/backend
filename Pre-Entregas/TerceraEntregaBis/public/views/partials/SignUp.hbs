  <div class="row">
  
  <div class="form col s12">

    <form action="/register" method="post">
  
      <span>Registro</span>
      <div class="input-field col s12">
     <input name="email" id="email" type="email" placeholder="email" required/>
     <input name="password" id="password" type="text" placeholder="password" required/>
      <input name="name" id="name" type="text" placeholder="name" required/>
       <input name="surname" id="surname" type="text" placeholder="Surname" />
        <input name="address" id="address" type="text" placeholder="address" />
         <input name="age" id="age" type="text" placeholder="age" />
          <input name="phone" id="phone" type="text" placeholder="phone" />
          <input type="file" id="avatar">
            <img src="" id="preview"></img>
 
          <button type="submit">
          SignUp
        </button>
        </div>
    </form>  

  </div>
</div>